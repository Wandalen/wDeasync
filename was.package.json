{
  "name": "wdeasync",
  "version": "0.1.20",
  "description": "Turns async function into sync via JavaScript wrapper of Node event loop",
  "main": "index.js",
  "author": "Vladimir Kurchatkin <vladimir.kurchatkin@gmail.com>",
  "contributors": [
    "Fred Wen <wenfred@gmail.com> (https://github.com/abbr)"
  ],
  "license": "MIT",
  "scripts": {
    "install" : "node-pre-gyp install --update-binary --fallback-to-build",
    "node-pre-gyp-install" : "node-pre-gyp install --update-binary --fallback-to-build",
    "node-pre-gyp-build" : "node-pre-gyp configure && node-pre-gyp rebuild",
    "node-pre-gyp-package" : "node-pre-gyp package",
    "node-pre-gyp-github-release" : "node-pre-gyp-github publish --release",
    "test": "node spec"
  },
  "files" : [ "binding.gyp", "src", "index.js" ],
  "binary" :
  {
    "module_name" : "deasync",
    "module_path" : "./binding/",
    "host" : "https://github.com/Wandalen/wDeasync/releases/download/",
    "remote_path" : "{version}"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^1.7.1",
    "node-pre-gyp" : "0.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wandalen/wDeasync.git"
  },
  "homepage": "https://github.com/Wandalen/wDeasync",
  "keywords": [
    "async",
    "sync",
    "sleep",
    "async wrapper"
  ],
  "engines": {
    "node": ">=0.11.0"
  },
  "devDependencies": {
    "require-directory": "^2.1.1"
  }
}
